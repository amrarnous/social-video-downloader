# Production Environment Variables
NODE_ENV=production
PORT=3001

# API Configuration
API_PREFIX=api
API_VERSION=v1

# Rate Limiting
RATE_LIMIT_TTL=60
RATE_LIMIT_LIMIT=50

# CORS Configuration
CORS_ORIGIN=https://yourdomain.com

# Download Configuration
MAX_DOWNLOAD_SIZE=500MB
DOWNLOAD_TIMEOUT=300000

# Video Quality Settings
DEFAULT_VIDEO_QUALITY=720p
SUPPORTED_FORMATS=mp4,webm

# Cache Configuration (if using Redis)
REDIS_HOST=redis
REDIS_PORT=6379
REDIS_TTL=3600

# Logging
LOG_LEVEL=info

# Security
HELMET_ENABLED=true
